<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
<h1>Form</h1>

<form>
 

  <div class="form-group row">
    <label for="inputText" class="col-sm-2 col-form-label">Ad Soyad</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" id="inputText" placeholder="Lütfen Giriniz" required>
    </div>
  </div>

  <div class="form-group row">
    <label for="inputEmail" class="col-sm-2 col-form-label mt-3">Email<br></label>
    <div class="col-sm-10 mt-3">
      <input type="email" class="form-control" id="inputEmail" placeholder="Lütfen Giriniz" required>
    </div>
  </div>

  <div class="form-group row">
    <label for="inputTel" class="col-sm-2 col-form-label mt-3">Telefon Numarası<br></label>
    <div class="col-sm-10 mt-3">
      <input type="tel" class="form-control" id="inputTel" placeholder="Lütfen Giriniz" required>
    </div>
  </div>

  <div class="form-group row">
    <label for="inputDate" class="col-sm-2 col-form-label mt-3">Doğum Tarihi<br></label>
    <div class="col-sm-10 mt-3">
      <input type="date" class="form-control" id="inputDate" placeholder="Lütfen Giriniz" required>
    </div>
  </div>

  <div class="col-form-label form-group mt-3">
    <label for="Select1">Doğum Yeri<br></label>
    <select class="form-control" id="Select">
      <option>İstanbul</option>
      <option>Ankara</option>
      <option>Malatya</option>
      <option>Adana</option>
      <option>İzmir</option>
    </select>
  </div>


  <div class="form-group">
    <label for="inputGonder" class="col-sm-2 col-form-label mt-3"><br></label>
    <div class="col-sm-1 mt-5">
      <input type="submit" class="form-control bg-success" id="inputGonder">
    </div>
  </div>

  </form>
